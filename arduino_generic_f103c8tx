#ifndef ARDUINO_GENERIC_F103C8TX_H
#define ARDUINO_GENERIC_F103C8TX_H

#include <Arduino.h>
#include "globals.h"

#define BOARD_TYPE F("STM32F103C8Tx")
pinDefinition pinMap[TOTAL_PINS] = {
  {PB11,DIOP},{PB10,DIOP},{PB1,AIDIOP},{PB0,AIDIOP},{PA7,AIDIOP},{PA6,AIDIOP},
  {PA5,AIDIO},{PA4,AIDIO}
};
#define I2C_SDA A4
#define I2C_SCL A5

#endif